{
    "manifest_version": 2,
    "name": "Chrome word api",
    "version": "0.1",
    "content_scripts": [{
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
],
"background": {
    "scripts":["background.js"]
},
"browser_action": {
    "default_icon": "buttons.png",
    "default_popup": "sketch/index.html",
    "default_title": "lookup a definition"
}
  
  
  }