{   
    "manifest_version": 2,
    "name": "RaterFox",
    "description": "The most popular movies and TV shows in your   default tab. Includes ratings",
    "version": "0.1",
    "browser_action": {
           "default_icon": "films.png",
           "default_title": "Have a good day",
           "default_popup": "popup.html"
    },
    "chrome_url_overrides":{
          "newtab": "newtab.html"
        },
        "permissions": ["activeTab"]
    }

